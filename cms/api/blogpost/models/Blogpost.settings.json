{
  "connection": "default",
  "collectionName": "blogposts",
  "info": {
    "name": "blogpost",
    "description": "template for a blogpost"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Paragraphs": {
      "type": "text"
    },
    "MediaContent": {
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    },
    "author": {
      "type": "string"
    }
  }
}